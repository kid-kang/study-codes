{
  "pages": [
    {
      "path": "pages/home/index",
      "style": {
        "navigationBarTitleText": "列表",
        "enablePullDownRefresh": false
      }
    },
    {
      "path": "pages/module/index",
      "style": {
        "navigationBarTitleText": "模块",
        "enablePullDownRefresh": false
      }
    },
    {
      "path": "pages/form/index",
      "style": {
        "navigationBarTitleText": "表单",
        "enablePullDownRefresh": false
      }
    }
  ],
  "globalStyle": {
    "navigationBarTextStyle": "black",
    "navigationBarTitleText": "uni-app",
    "navigationBarBackgroundColor": "#F8F8F8",
    "backgroundColor": "#F8F8F8"
  },
  // 分包页面
  "subPackages": [
    {
      "root": "subpkgs/login",
      "name": "login",
      "pages": [
        {
          "path": "index",
          "style": {
            "navigationBarTitleText": "登录",
            "enablePullDownRefresh": false
          }
        }
      ]
    },
    {
      "root": "subpkgs/success",
      "name": "success",
      "pages": [
        {
          "path": "index",
          "style": {
            "navigationBarTitleText": "提交成功",
            "enablePullDownRefresh": false
          }
        }
      ]
    },
    {
      "root": "subpkgs/waterPrint",
      "name": "waterPrint",
      "pages": [
        {
          "path": "index",
          "style": {
            "navigationBarTitleText": "底部水印",
            "enablePullDownRefresh": false
          }
        }
      ]
    }
  ],
  // 预加载
  "preloadRule": {
    "pages/home/index": {
      "packages": ["login"]
    },
    "pages/form/index": {
      "packages": ["success"]
    },
    "pages/module/index": {
      "packages": ["waterPrint"]
    }
  },
  // 底部tabBar
  "tabBar": {
    "color": "#333",
    "backgroundColor": "#fff",
    "selectedColor": "#3787FF",
    "list": [
      {
        "pagePath": "pages/home/index",
        "text": "列表",
        "iconPath": "static/tab-icons/home.png",
        "selectedIconPath": "static/tab-icons/home_active.png"
      },
      {
        "pagePath": "pages/module/index",
        "text": "模块",
        "iconPath": "static/tab-icons/module.png",
        "selectedIconPath": "static/tab-icons/module_active.png"
      },
      {
        "pagePath": "pages/form/index",
        "text": "表单",
        "iconPath": "static/tab-icons/ucenter.png",
        "selectedIconPath": "static/tab-icons/ucenter_active.png"
      }
    ]
  }
}
